<h1>Upload a file</h1>
<div id="new-upload">

<%= render 'form' %>

</div>
<br/>
<br/>
<table class="table table-striped table-bordered"  id="uploads">
  <thead>
     <tr>
      <th scope="col">File Name</th>
      <th scope="col">AWS Link</th>
      <th scope="col">Added by</th>
      <th scope="col">Status</th>
      <th scope="col">Created At</th>
    </tr>
  </thead>
  <tbody>
<%= render @uploads %>
</tbody>
</table>